<html t:type="layout" title="PrviTapestryProjekat com.example"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
    <div class="row">
        <div class="span4 offset3">
            <t:beaneditform t:id="form" object="korisnikLogin" submitlabel="Login" exclude="iDKorisnik,rola,ime,prezime,adresa">
                <t:errors/>
                <p:lozinka>
                    <t:label for="lozinka"/>
                    <t:passwordfield t:id="lozinka" value="korisnikLogin.lozinka"/>
                </p:lozinka>
            </t:beaneditform>
        </div>
    </div>
</html>